<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_edit_plan"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jake.weatherandroidapp.activity.EditPlan">

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        app:layout_constraintLeft_toLeftOf="@+id/activity_edit_plan"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="48dp"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        android:layout_marginTop="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_edit_plan"
        android:layout_marginBottom="16dp" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        app:layout_constraintLeft_toRightOf="@+id/button6"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        android:layout_marginTop="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_edit_plan"
        android:layout_marginBottom="16dp" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        app:layout_constraintLeft_toRightOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        android:layout_marginTop="56dp"
        app:layout_constraintRight_toRightOf="@+id/activity_edit_plan"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_edit_plan"
        android:layout_marginBottom="16dp"
        app:layout_constraintHorizontal_bias="0.61" />

    <EditText
        android:layout_width="384dp"
        android:layout_height="328dp"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:id="@+id/editText3"
        app:layout_constraintLeft_toLeftOf="@+id/activity_edit_plan"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        app:layout_constraintTop_toTopOf="@+id/activity_edit_plan"
        android:layout_marginTop="5dp"
        app:layout_constraintRight_toRightOf="@+id/activity_edit_plan"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_edit_plan"
        android:layout_marginBottom="15dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="0.2" />
</android.support.constraint.ConstraintLayout>
