<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_change_city"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jake.weatherandroidapp.activity.ChangeCityActivity">

    <Spinner
        android:layout_width="352dp"
        android:layout_height="32dp"
        android:id="@+id/spinner"
        app:layout_constraintLeft_toLeftOf="@+id/activity_change_city"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/activity_change_city"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="@+id/activity_change_city"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_change_city"
        android:layout_marginBottom="16dp"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintVertical_bias="0.25" />

    <Spinner
        android:layout_width="352dp"
        android:layout_height="32dp"
        android:id="@+id/spinner2"
        app:layout_constraintLeft_toLeftOf="@+id/activity_change_city"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="@+id/activity_change_city"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_change_city"
        android:layout_marginBottom="16dp"
        app:layout_constraintHorizontal_bias="0.57"
        app:layout_constraintVertical_bias="0.060000002" />

    <Button
        android:text="Button"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:id="@+id/button3"
        app:layout_constraintLeft_toLeftOf="@+id/activity_change_city"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner2"
        app:layout_constraintRight_toRightOf="@+id/activity_change_city"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_change_city"
        android:layout_marginBottom="16dp" />
</android.support.constraint.ConstraintLayout>
